{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Alarm Clock - on & offline",
    "description":"Alarm Clock that runs both on and off line as well as in the background",
    "version": "0.0.0.7",
    "minimum_chrome_version": "23",
    "icons": {
        "16": "icon_16.png",
        "32": "icon_32.png",
        "64": "icon_64.png",
        "128": "icon_128.png",
        "256": "icon_256.png"
    },
    "app": {
        "background": {
            "scripts": [
                "app.js"
            ],
            "persistent": true
        }
    },
    "permissions":[
        "power"
    ]
}
