{
    "manifest_version": 2,
    "name": "Alarm Clock - on & offline Free",
    "short_name" : "Free Alarm",
    "description":"Alarm Clock that runs both on and off line as well as in the background",
    "version": "1.0.0.0",
    "offline_enabled": true,
    "minimum_chrome_version": "33",
    "icons": {
        "16": "app/resources/icons/-16.png",
        "32": "app/resources/icons/-32.png",
        "64": "app/resources/icons/-64.png",
        "128": "app/resources/icons/-128.png",
        "256": "app/resources/icons/-256.png"
    },
    "app": {
        "background": {
            "scripts": [
                "main.js"
            ]
        }
    },
    "permissions":[
        "power",
        "storage"
    ]
}
